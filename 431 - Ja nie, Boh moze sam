  <title>Ja nie, Boh môže sám</title>
  <author></author>
  <copyright>Praha 1971 Bratská jednota baptistov</copyright>
  <hymn_number>431</hymn_number>
  <presentation></presentation>
  <ccli></ccli>
  <capo print="false"></capo>
